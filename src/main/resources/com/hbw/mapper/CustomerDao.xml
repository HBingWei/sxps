<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbw.dao.CustomerDao">

    <select id="queryByAccount" resultType="com.hbw.entity.Customer">
        select id, account, name, password, lasttime, tel, email, sex, authid
            from customer
            where account = #{account}
    </select>

</mapper>

