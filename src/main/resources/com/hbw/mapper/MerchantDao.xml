<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbw.dao.MerchantDao">

    <select id="queryByAccount" resultType="com.hbw.entity.Merchant">
        select id, account, name, password, lasttime, tel, email, sex, authid
        from merchant
        where account = #{account}
    </select>

</mapper>

