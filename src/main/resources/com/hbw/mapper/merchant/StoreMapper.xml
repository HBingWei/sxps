<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbw.dao.StoreDao">

    <select id="getByMerId" resultType="com.hbw.entity.Store">
        select id, merid, name, address, tel, about, state
        from store
        where merid = #{merid}
    </select>

</mapper>

